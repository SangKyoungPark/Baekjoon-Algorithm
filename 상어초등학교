#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
using namespace std;

#define SIZE 22

int n;
int map[SIZE][SIZE];
int info[SIZE][5];

int dy[] = { 0,0,1,-1 };
int dx[] = { 1,-1,0,0 };

int getScore(int y, int x) {
	// 상하좌우 좋아하는 사람 찾아서 점수 계산
	// 0(0) / 1(1) / 2(10) / 3(100) / 4(1000)
	int idx = 0;
	for (int i = 0; i < n * n; i++) {
		if (map[y][x] == info[i][0]) {
			idx = i;
			break;
		}
	}

	int cnt = 0;
	for (int i = 0; i < 4; i++) {
		int ny = dy[i] + y;
		int nx = dx[i] + x;
		if (ny < 0 || ny >= n || nx < 0 || nx >= n) continue;
		for (int j = 1; j < 5; j++) {
			if (map[ny][nx] == info[idx][j]) cnt++;
		}
	}
	if (cnt <= 1) return cnt;
	else return (cnt - 1) * 10;
}

int solve() {
	int result = 0;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			result += getScore(i,j);
		}
	}
}

int main() {
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < 5; j++) {
			scanf("%d",&info[i][j]);
		}
	}

	//logic
	int res = solve();

	//result
	printf("%d", res);

	return 0;
}
